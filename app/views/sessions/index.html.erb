<h1>Listing sessions</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Location</th>
    <th>Speaker</th>
    <th>Helper</th>
    <th>Institution </th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sessions.each do |session| %>
  <tr>
    <td><%= session.title %></td>
    <td><%= session.description %></td>
    <td><%= session.location_id %></td>
    <td><%= session.speaker_id %></td>
    <td><%= session.helper_id %></td>
    <td><%= session.institution_id %></td>
    <td><%= link_to 'Show', session %></td>
    <td><%= link_to 'Edit', edit_session_path(session) %></td>
    <td><%= link_to 'Destroy', session, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Session', new_session_path %>
